{
  "name": "hublocal-back",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev ./src/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli -d ormconfig.ts",
    "migration:create": "cross-var ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/infra/migrations/$npm_config_name",
    "migration:run": "npm run build && npm run typeorm -- migration:run"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-var": "^1.1.0",
    "express": "^4.18.2",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.11"
  }
}
